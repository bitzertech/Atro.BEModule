{
  "type": "Base",
  "name": "Motor",
  "label": "Motor",
  "label_plural": "Motors",
  "fields": {
    "code": { "type": "varchar", "required": true, "unique": true, "maxLength": 190 },
    "name": { "type": "varchar", "required": true, "maxLength": 190 },
    "code_and_connection": { "type": "varchar", "required": true, "label": "Code + Connection" },
    "voltage": { "type": "int", "label": "Voltage" },
    "frequency": { "type": "int", "label": "Frequency" },
    "spkz": { "type": "int", "label": "SPKZ" },
    "motor_code": { "type": "varchar", "required": true, "label": "Motor Code" },

    "voltage_range": { "type": "varchar", "maxLength": 30, "label": "Voltage Range" },
    "connection": { "type": "varchar", "maxLength": 5, "label": "Connection" },

    "voltage_frequency_factor": { "type": "float", "label": "Voltage Frequency Factor" },
    "toggle_value": { "type": "int", "label": "Toggle Value" },

    "createdAt": { "type": "datetime", "readOnly": true },
    "modifiedAt": { "type": "datetime", "readOnly": true },
    "createdBy": { "type": "link", "readOnly": true, "view": "views/fields/user" },
    "modifiedBy": { "type": "link", "readOnly": true, "view": "views/fields/user" }
  },
  "links": {
    "createdBy": { "type": "belongsTo", "entity": "User" },
    "modifiedBy": { "type": "belongsTo", "entity": "User" },

    "products": {
      "type": "manyMany",
      "entity": "Product",
      "relationName": "productMotor"
    }
  },
  "collection": {
    "textFilterFields": ["name", "code"],
    "sortBy": "createdAt",
    "asc": false
  },
  "indexes": {
    "name": { "columns": ["name", "deleted"] },
    "code": { "columns": ["code", "deleted"] }
  }}
