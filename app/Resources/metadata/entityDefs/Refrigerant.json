{
  "type": "Base",
  "name": "Refrigerant",
  "label": "Refrigerant",
  "label_plural": "Refrigerants",
  "fields": {
    "code": { "type": "varchar", "required": true, "unique": true, "maxLength": 190 },
    "name": { "type": "varchar", "required": true, "maxLength": 190 },
    
    "number": { "type": "int", "required": true, "label": "Number" },
    "critical_pr": { "type": "float", "required": true, "label": "Critical Pressure" },
    "critical_temp": { "type": "float", "required": true, "label": "Critical Temperature" },
    "pr_range_start": { "type": "float", "required": true, "label": "Pressure Range Start" },
    "pr_range_end": { "type": "float", "required": true, "label": "Pressure Range End" },
    "dew_pr_change": { "type": "float", "required": true, "label": "Dew Point Pressure Change" },
    "dew_temp_change": { "type": "float", "required": true, "label": "Dew Point Temperature Change" },
    "dew_a1": { "type": "float", "required": true, "label": "Dew A1" },
    "dew_b1": { "type": "float", "required": true, "label": "Dew B1" },
    "dew_c1": { "type": "float", "required": true, "label": "Dew C1" },
    "dew_a2": { "type": "float", "required": true, "label": "Dew A2" },
    "dew_b2": { "type": "float", "required": true, "label": "Dew B2" },
    "dew_c2": { "type": "float", "required": true, "label": "Dew C2" },
    "bubble_pr_change": { "type": "float", "required": true, "label": "Bubble Point Pressure Change" },
    "bubble_temp_change": { "type": "float", "required": true, "label": "Bubble Point Temperature Change" },
    "bubble_a1": { "type": "float", "required": true, "label": "Bubble A1" },
    "bubble_b1": { "type": "float", "required": true, "label": "Bubble B1" },
    "bubble_c1": { "type": "float", "required": true, "label": "Bubble C1" },
    "bubble_a2": { "type": "float", "required": true, "label": "Bubble A2" },
    "bubble_b2": { "type": "float", "required": true, "label": "Bubble B2" },
    "bubble_c2": { "type": "float", "required": true, "label": "Bubble C2" },

    "createdAt": { "type": "datetime", "readOnly": true },
    "modifiedAt": { "type": "datetime", "readOnly": true },
    "createdBy": { "type": "link", "readOnly": true, "view": "views/fields/user" },
    "modifiedBy": { "type": "link", "readOnly": true, "view": "views/fields/user" }
  },
  "links": {
    "createdBy": { "type": "belongsTo", "entity": "User" },
    "modifiedBy": { "type": "belongsTo", "entity": "User" },

    "products": {
      "type": "manyMany",
      "entity": "ProductRefrigerantOilType",
      "relationName": "productRefrigerantOilType"
    }
  },
  "collection": {
    "textFilterFields": ["name", "code"],
    "sortBy": "createdAt",
    "asc": false
  },
  "indexes": {
    "name": { "columns": ["name", "deleted"] },
    "code": { "columns": ["code", "deleted"] }
  }}
