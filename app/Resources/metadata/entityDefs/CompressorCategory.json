{
  "fields": {
    "name": {
      "type": "varchar",
      "required": true,
      "trim": true,
      "isMultilang": true
    },
    "description": {
      "type": "text",
      "rowsMax": 4,
      "lengthOfCut": 400,
      "isMultilang": true
    },
    "code": {
      "type": "varchar",
      "trim": true,
      "unique": true
    },
    "mainImage": {
      "type": "file",
      "previewSize": "medium",
      "notStorable": true,
      "readOnly": true,
      "massUpdateDisabled": true,
      "filterDisabled": true,
      "emHidden": true
    },
    "files": {
      "type": "linkMultiple",
      "layoutDetailDisabled": true,
      "massUpdateDisabled": true,
      "noLoad": true
    },
    "isActive": {
      "notNull": true,
      "type": "bool"
    },
    "createdAt": {
      "type": "datetime",
      "readOnly": true
    },
    "modifiedAt": {
      "type": "datetime",
      "readOnly": true
    },
    "createdBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "modifiedBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "ownerUser": {
      "type": "link",
      "required": true,
      "view": "views/fields/owner-user"
    },
    "assignedUser": {
      "type": "link",
      "required": true,
      "view": "views/fields/assigned-user"
    },
    "teams": {
      "type": "linkMultiple",
      "view": "views/fields/teams"
    },
    "sortOrder": {
      "type": "int",
      "default": null,
      "massUpdateDisabled": true,
      "filterDisabled": true,
      "exportDisabled": true
    },
    "_position": {
      "type": "varchar",
      "notStorable": true,
      "layoutListDisabled": true,
      "layoutListSmallDisabled": true,
      "layoutDetailDisabled": true,
      "layoutDetailSmallDisabled": true,
      "massUpdateDisabled": true,
      "filterDisabled": true,
      "importDisabled": true,
      "exportDisabled": true,
      "emHidden": true
    },
    "_target": {
      "type": "varchar",
      "notStorable": true,
      "layoutListDisabled": true,
      "layoutListSmallDisabled": true,
      "layoutDetailDisabled": true,
      "layoutDetailSmallDisabled": true,
      "massUpdateDisabled": true,
      "filterDisabled": true,
      "importDisabled": true,
      "exportDisabled": true,
      "emHidden": true
    },
    "childrenCount": {
      "type": "int",
      "notStorable": true,
      "layoutListDisabled": true,
      "layoutListSmallDisabled": true,
      "layoutDetailDisabled": true,
      "layoutDetailSmallDisabled": true,
      "massUpdateDisabled": true,
      "filterDisabled": true,
      "importDisabled": true,
      "exportDisabled": true
    },
    "categoryRoute": {
      "type": "text",
      "layoutListDisabled": true,
      "layoutDetailDisabled": true,
      "massUpdateDisabled": true,
      "filterDisabled": false,
      "importDisabled": true
    },
    "categoryRouteName": {
      "type": "text",
      "layoutDetailDisabled": true,
      "massUpdateDisabled": true,
      "filterDisabled": false,
      "importDisabled": true
    },
    "hasChildren": {
      "type": "bool",
      "notStorable": true,
      "default": false,
      "layoutListDisabled": true,
      "layoutDetailDisabled": true,
      "massUpdateDisabled": true,
      "filterDisabled": true,
      "importDisabled": true,
      "exportDisabled": true
    },
    "compressors": {
      "type": "linkMultiple",
      "layoutDetailDisabled": true,
      "massUpdateDisabled": true,
      "noLoad": true
    },
    "children": {
      "type": "linkMultiple",
      "layoutDetailDisabled": true,
      "noLoad": true
    },
    "parents": {
      "type": "linkMultiple",
      "view": "views/fields/hierarchy-parents"
    }
  },
  "links": {
    "files": {
      "type": "hasMany",
      "relationName": "categoryFile",
      "foreign": "compressorCategories",
      "entity": "File"
    },
    "createdBy": {
      "type": "belongsTo",
      "entity": "User"
    },
    "modifiedBy": {
      "type": "belongsTo",
      "entity": "User"
    },
    "ownerUser": {
      "type": "belongsTo",
      "entity": "User"
    },
    "assignedUser": {
      "type": "belongsTo",
      "entity": "User"
    },
    "teams": {
      "type": "hasMany",
      "entity": "Team",
      "relationName": "EntityTeam",
      "layoutRelationshipsDisabled": true
    },
    "children": {
      "type": "hasMany",
      "relationName": "CategoryHierarchy",
      "foreign": "parents",
      "entity": "CompressorCategory",
      "midKeys": [
        "entityId",
        "parentId"
      ]
    },
    "parents": {
      "type": "hasMany",
      "relationName": "CategoryHierarchy",
      "foreign": "children",
      "entity": "CompressorCategory",
      "midKeys": [
        "parentId",
        "entityId"
      ]
    },
    "compressors": {
      "type": "hasMany",
      "relationName": "compressorCategory",
      "foreign": "compressorCategories",
      "entity": "Compressor"
    }
  },
  "collection": {
    "sortBy": "createdAt",
    "asc": false,
    "textFilterFields": [
      "name",
      "code"
    ]
  },
  "indexes": {
    "name": {
      "columns": [
        "deleted"
      ]
    },
    "ownerUser": {
      "columns": [
        "ownerUserId",
        "deleted"
      ]
    },
    "assignedUser": {
      "columns": [
        "assignedUserId",
        "deleted"
      ]
    }
  }
}
